bundle Default {
	class Thingy {
		a : Int;
		b : static : Int;
		c : Float;
		
		New() {
			a := 13;
			b := 7;
			c := a / b;
		}

		method : public : Get() ~ Int {
			return b;
		}
	}

	class Test {
		function : Main(args : String[]) ~ Nil {
			t := Thingy->New();
			t->Get()->PrintLine();
		}
	}
}

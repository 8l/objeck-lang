bundle Default {
	class Hello {
		function : Main(args : String[]) ~ Nil {
			i := 10;
			while(i <> 0) {
				select(i) {
					label 3: {
						x := 100;
						j := 0;
						while(j < 3) {
							j += 1;
						};
					}

					label 1: {
						j := 0;
						while(j < 6) {
							j->PrintLine();
							j += 1;
						};
					}
				};
				i -= 1;
				i := x;
			};	
		}
	}
}

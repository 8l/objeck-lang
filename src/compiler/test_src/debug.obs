use IO;

bundle Default {
	class Foo {
		function : Main(args : String[]) ~ Nil {
			dx := 2.4;	
			dy := 3.3;	
			dz := 4.2;	

			a := dx * dx + dy * dy + dz * dz;
			b := a->SquareRoot() / 2.0;
			b->PrintLine();

			x := (dx * dx + dy * dy + dz * dz)->SquareRoot() / 2.0;
			x->PrintLine();
		}
	}
}

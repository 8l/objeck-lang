class Foo {
	@id : Int;
	New() { @id := 13; }
}

class Bar from Foo {
# class Bar {
	@id : Int;
	New() { Parent(); @id := 7; }

	method : public : GetId() ~ Int {
		@id := 8;
		return @parent->@id;
	}
}

class Test {
	function : Main(args : String[]) ~ Nil {
		b := Bar->New();
		b->GetId()->PrintLine();
	}
}


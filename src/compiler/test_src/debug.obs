use RegEx;
use System.IO.File;

class RegExTest {
	function : Main(args : String[]) ~ Nil {
		input := FileReader->ReadFile(args[0]);
		input->PrintLine();

		expr := "<(a|A)\\s(href|HREF)=\"?http://(\\w*\\.\\w*\\.\\w*|\\w*\\.\\w*|\\w*)(/\\w*)?\"";
		found := RegEx->New(expr)->Find(input);
		each(i : found) {
			found->Get(i)->As(String)->PrintLine();
		};
	}
}

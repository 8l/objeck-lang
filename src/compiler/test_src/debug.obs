bundle Default {
	class Debug {
		function : Main(args : String[]) ~ Nil {
			name := "in.txt";

			t := IO.File->CreateTime(name, false);
			t->ToString()->PrintLine();

			IO.File->ModifiedTime(name, false)->ToString()->PrintLine();
			IO.File->AccessedTime(name)->ToString()->PrintLine();
		}
	}
}

class RowEchelon {
	function : Main(args : String[]) ~ Nil {
#~
		in := "Rosetta code"->ToByteArray();
		out := Encryption.Hash->SHA256(in);

		if(out <> Nil) {
			a  := out[1]->As(Int);
			a->PrintLine();

			b := out[16]->As(Int);
			b->PrintLine();
		};
~#
			key := "Hello World!"->ToByteArray();
			in := "Rosetta code"->ToByteArray();
			out := Encryption.Encrypt->AES256(key, in);

			a := out[1]->As(Int);
			a->PrintLine();

			a := out[8]->As(Int);
			a->PrintLine();

			out2 := Encryption.Decrypt->AES256(key, out);

			c := out2[0]->As(Char);
			c->PrintLine();

			c := out2[1]->As(Char);
			c->PrintLine();

			c := out2[8]->As(Char);
			c->PrintLine();

			out3 := String->New(out2);
			out3->PrintLine();
	}	
}

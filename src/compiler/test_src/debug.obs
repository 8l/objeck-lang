bundle Default {
	class Thingy {
		a : Int;
		b : static : Int;
		c : Float;
		
		New() {
			a := 13;
			b := 7;
			c := a / b;
		}

		method : public : Get() ~ Int {
			return b;
		}
	}

	class Test {
		function : DoStuff(postfix : String) ~ Nil {
			stack := Struct.Stack->New();

			each(i : postfix) {
				char := postfix->Get(i);
				select(char) {
					other: {
						if(char = '.') {
							"is '.'"->PrintLine();
						}
						else {
							"not '.'"->PrintLine();
						};
					}

					label 'b': {  
						"is 'b'"->PrintLine();
					}

					label 'c': {  
						"is 'c'"->PrintLine();
					}
				};
			};
		}

#~
		function : Main(args : String[]) ~ Nil {
			t := Thingy->New();
			t->Get()->PrintLine();

			DoStuff("ab.cd");
		}
~#
	}
}

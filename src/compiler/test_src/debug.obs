class Test {
	@a : String;
	@b : String;

	New() {
		@a := "a b c";
		@b := "c d e";
	}
	
	function : Main(args : String[]) ~ Nil {
		Test->New()->Run();
	}

	method : Run() ~ Nil {
		a := "1 2 3";
		a += "4 5 6";
		a += @b;
		@b += a;
		@b += @a;

		a->PrintLine();
		@b->PrintLine();
		a := "1 2 3";
		a += "4 5 6";
		a->PrintLine();
	}
}

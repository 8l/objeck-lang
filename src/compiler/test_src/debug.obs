		return 33;
	}

	function : Fie(a : Int := 7, b : Int := Foo()) ~ Int {
		return a+b;
	}

	function : Fum(a : Int, b : Int := a, c : Int := 7) ~ Nil {
		(a*b*c)->PrintLine();
	}


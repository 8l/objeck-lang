class RowEchelon {
	function : Main(args : String[]) ~ Nil {
#~
		in := "Rosetta code"->ToByteArray();
		out := Encryption.Hash->SHA256(in);

		if(out <> Nil) {
			a  := out[1]->As(Int);
			a->PrintLine();

			b := out[16]->As(Int);
			b->PrintLine();
		};
~#
		key := "Forest Room 5"->ToByteArray();
		in := "Eighteen carat gold pen when it hits the sheets / Words worth a million like I'm rapping 'em through platinum teeth."->ToByteArray();
		encrypted := Encryption.Encrypt->AES256(key, in);
		if(encrypted <> Nil) {
			decprypted := Encryption.Decrypt->AES256(key, out);
			if(decprypted <> Nil) {
				String->New(decprypted)->PrintLine();
			};
		};
	}	
}

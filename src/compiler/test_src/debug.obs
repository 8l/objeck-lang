class Foo {
	@id : Int;
	New() { @id := 13; }
}

class Bar from Foo {
	@id : Int;

	New() { Parent(); @id := 7; }

	method : public : GetParentId() ~ Int {
		return @parent->@id;
	}

	method : public : GetId() ~ Int {
		return @id;
	}
}

class Test {
	function : Main(args : String[]) ~ Nil {
		b := Bar->New();
		b->GetParentId()->PrintLine();
		b->GetId()->PrintLine();
	}
}


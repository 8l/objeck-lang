class Bar {
	@value : String;
	@s_value : static : String;

	New() { @value := "Hello"; }

	method : public : Value() ~ String {
		return @value;
	}

	function : Set() ~ Nil {
		@s_value := "im static";
	}
}
	
class Test {
	New() {}

	function : Foo(s : String) ~ Bar {
		return Bar->New();
	}

	function : Main(args : String[]) ~ Nil {
		Run()->PrintLine();
	}

	function : Run() ~ String {
		value := 3.157;
		Bar->Set();
		t := Test->New();
		value->PrintLine();
		return t->Foo("bar")->Value();
	}
}


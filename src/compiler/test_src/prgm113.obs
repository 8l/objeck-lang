use IO;

bundle RegEx {
	enum TokenType := -32 {
		ZERO_MORE,
		ONE_MORE,
		ZERO_ONE,
		CONCAT,
		ALTERN
	}
	
	class RegEx {
		@input : String;
		@index : Int;

		New(input : String) {
			@input := input;
		}

		method : public : Parse() ~ Int[] {
		}

		function : Main(args : String[]) ~ Nil {
			regex := RegEx->New(args[0]);
			regex->PostFix();
		}
	}
}

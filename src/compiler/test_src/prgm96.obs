use IO;

bundle Default {
	class BasicBlocks {
		function : Main(args : System.String[]) ~ Nil {
			msg := "Hello";
			serial := Serializer->New();
			serial->Add(msg->As(Base));
			bytes := serial->Serialize();
			bytes->Size()->PrintLine();

			deserial := Deserializer->New(bytes);
			value := deserial->ReadObject()->As(String);
			value->PrintLine();
		}
	}
}

bundle Default {
	class Foo {
		a : Int;
		b : Int;

		New() {
			a := 7;
			b := 13;
		}

		method : public : fA() ~ Int {
			return a * 5 - b;
		}

		method : public : fB() ~ Int {
			return b - 5;
		}
	}
	
	class Test {
		function : f1() ~ Int {
			a := 5;
			return a * 2 + 5;
		}
	
		function : Run() ~ Nil {
			f := Foo->New();
			f->fA()->PrintLine();
			f->fA()->PrintLine();
			f->fB()->PrintLine();
			a := f1() + f1();
			a->PrintLine();

			f1 := Foo->New();
			f1->fA()->PrintLine();
		}

		function : Main(args : String[]) ~ Nil {
			Run();
		}
	}
}

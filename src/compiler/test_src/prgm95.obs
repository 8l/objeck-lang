		e := 2;
		c := Foo->New() {
			@m : Int;
			New() { @m := 31; }

			method : public : Go(a : Int) ~ Float {
				return e * 3.145;
			}
		};

		d := Foo->New() {
			New() {}
			method : public : Go(a : Int) ~ Float {
				return a * 67.95;

bundle Default {
	class Hello {
		New() {
		}

		function : Main(args : System.String[]), Nil {
			h := "Hello World!";
			h->SubString(3, 3)->PrintLine();
			h->SubString(0, 3)->PrintLine();
			

			Run(args);
		}

#		function : native : Run(args : String[]), Nil {
		function : Run(args : String[]), Nil {
			t := Time->New();
			t->GetHours()->Print(); ":"->Print(); t->GetMinutes()->Print(); ":"->Print(); t->GetSeconds()->PrintLine();
		}
	}
}

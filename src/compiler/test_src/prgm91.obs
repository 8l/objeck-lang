use IO;

bundle Default {
	class Hello {
		function : Main(args : String[]) ~ Nil {
			s := "Hello";
			c := s->GetClass();
			c->GetName()->PrintLine();
			m := c->GetMethods();
			each(i : m) {
				n := m[i];
				n->GetName()->PrintLine();
				b := n->GetName()->Equals("New");
				Console->Print("const=")->PrintLine(b);

				p := n->GetParameters();
				each(j : p) {
					a := p[j];
					Console->Print("\ttype=")->PrintLine(a->GetType()->As(Int));
					if(a->GetType() = TypeId->CLASS) {
						Console->Print("\tname=")->PrintLine(a->GetClassName());
					};
				};
				"#####"->PrintLine();
			};
		}
	}
}

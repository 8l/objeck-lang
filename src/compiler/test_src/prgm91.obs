use IO;

bundle Default {
	class Hello {
		function : Main(args : String[]) ~ Nil {
			s := "Hello";
			c := s->GetClass();
			c->GetName()->PrintLine();
			m := c->GetMethods();
			each(i : m) {
				n := m[i];
				n->GetName()->PrintLine();
				p := n->GetParameters();
				p->Size()->PrintLine();
"#####"->PrintLine();
				each(j : p) {
					a := p[j];
					Console->Print("\ttype: ")->PrintLine(a->GetType()->As(Int));
					if(a->GetType() = TypeId->CLASS) {
						Console->Print("\tname=")->PrintLine(a->GetClassName());
					};
				};
			};
		}
	}
}

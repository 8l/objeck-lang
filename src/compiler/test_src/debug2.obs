use Struct;

bundle Default {
	class Car {
		New() {}

		method : public : Foo(i : Int) ~ Nil {
			"int"->PrintLine();
		}
	}

	class Ford from Car {
		New() { Parent(); }

		method : public : Foo(s : String) ~ Nil {
			"string"->PrintLine();
		}
	}

	class Bike {
		New() { Parent(); }
	}

	class Plane {
		New() { Parent(); }
	}

	class Hello {
		function : Main(args : String[]) ~ Nil {
			f := Ford->New();
			f->Foo("foo");
		}
	}
}

use Struct;

bundle Default {
	class Hello {
		im : IntMap;

		New() {
			im := IntMap->New();
		}

		method : Test() ~ Nil {
			s := "Foo";
			im->Insert(42, s->As(Base));
		}
		
		method : Test2(n : Int) ~ Nil {
			s := im->Find(n);
		}

		function : Main(args : String[]) ~ Nil {
			h := Hello->New();
			h->Test();
			h->Test2(42);
		}
	}
}

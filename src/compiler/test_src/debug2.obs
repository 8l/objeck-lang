use Struct;

bundle Default {
	class Vehicle {
		New() {}
	}

	class Car from Vehicle {
		New() { Parent(); }

		method : public : Foo(i : Int) ~ Nil {
			"int"->PrintLine();
		}
	}

	class Ford from Car {
		New() { Parent(); }

		method : public : Foo(s : String) ~ Nil {
			"string"->PrintLine();
			Foo(11.5);
		}

		method : public : Foo(f : Float) ~ Nil {
			"float"->PrintLine();
		}
	}

	class Bike {
		New() { Parent(); }

		method : public : Foo(v : Vector) ~ Nil {
			"vector"->PrintLine();
		}
			
		method : public : Foo(s : String) ~ Nil {
			"string"->PrintLine();
		}
	}

	class Plane {
		New() { Parent(); }
	}

	class Hello {
		function : Main(args : String[]) ~ Nil {
#			f := Ford->New();
#			f->Foo("randy");

#			v := Vector->New();
#			b := Bike->New();
#			b->Foo(v);
			10->PrintLine();
		}
	}
}

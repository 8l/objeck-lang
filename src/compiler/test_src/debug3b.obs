class Fie {
	enum Type { A, B, C, D }

	function : Bar(p : Type) ~ Nil {
		p->PrintLine();
	}
}

class Foo {
	enum Type { A, B, C, D }

	function : Bar(p : Type) ~ Nil {
		p->PrintLine();
	}

	function : Main(args : String[]) ~ Nil {
		Bar(Foo->Type->A);
		@self->Bar(Type->B);
		Fie->Bar(Fie->Type->C);
		@self->PrintLine();
  }

	
	function : PrintLine() ~ Nil {
		"Hello"->PrintLine();
	}
}

bundle Default {
	class Hello {
		function : Main(args : System.String[]), Nil {
			for(i := 1; i <= 10; i := i + 1;) { 
				"-------------------"->PrintLine();
				select(i) {
					label 5: {
						1->PrintLine();
					}
					
					label 2: {
						2->PrintLine();
					}
			
					label 33: {
						3->PrintLine();
					}
					
					label 14: {
						4->PrintLine();
					}
					
					label -5: {
						5->PrintLine();
					}
					
					label 6: {
						6->PrintLine();
					}
					
					label 27: {
						7->PrintLine();
					}
					
					label -8: {
						8->PrintLine();
					}
					
					label 9: {
						9->PrintLine();
					}
					
					label 10: {
						10->PrintLine();
					}
					
					other: {
						"other"->PrintLine();
					}
				};
				i->PrintLine();
			};	
		}
	}
}

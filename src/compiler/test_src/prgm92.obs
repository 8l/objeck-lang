bundle Default {
	class Hello {
		function : Main(args : String[]) ~ Nil {
			Run2();
			Run();
		}

		function : native : Run() ~ Nil {
			c := Class->New("System.String");
			c->IsLoaded()->PrintLine();
		}

		function : native : Run2() ~ Nil {
			s := "xxx";
			if(s->TypeOf(String)) {
				">> true <<"->PrintLine();
			}
			else {
				">> false <<"->PrintLine();
			};
		}
	}
}

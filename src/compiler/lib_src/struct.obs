	#~~~~~~~~~~~~~~~~~~~~~~~
	# Holders
	~~~~~~~~~~~~~~~~~~~~~~~#
	class IntHolder implements Compare {
		@value : Int;

		New() {
			Parent();
			@value := 0;
		}

		New(value : Int) {
			Parent();
			@value := value;
		}

		method : public : Get() ~ Int {
			return @value;
		}

		method : public : native : Compare(rhs : Compare) ~ Int {
			right : IntHolder := rhs->As(IntHolder);
			if(@value = right->Get()) {
				return 0;
			}

bundle Default {
	class Hello {
		possibilities : static : Char[];
		
		function : Init() ~ Nil {
			possibilities := "ABCDEFGHIJKLMNOPQRSTUVWXYZ "->ToCharArray();
		}
		
  		function : Main(args : String[]) ~ Nil {
			Init();
					
			for(i := 0; i < 100; i += 1;) {
				"poss: "->Print(); possibilities->Size()->PrintLine();
			};
		}
	}
}

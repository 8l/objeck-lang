use Net;
use Struct;

bundle Default {
  class HttpTest {
    function : Main(args : String[]) ~ Nil {
      client := HttpClient->New("http://www.mscd.edu", 80);
      lines := client->Get();
      
      for(i := 0; i < lines->Size(); i += 1;) {
        lines->Get(i)->As(String)->PrintLine();
      };
    }
  }
}

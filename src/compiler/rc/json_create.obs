use Struct;
use JSON;

bundle Default {
	class Json {
		function : Main(args : String[]) ~ Nil {
			array_values := Vector->New();
			array_values->AddBack(JSONElement->New("hello"));
			array_values->AddBack(JSONElement->New("world"));
			root := JSONElement->New(array_values);	
#			root->ToString()->PrintLine();

			map_values := StringMap->New();
			map_values->Insert("greetings", root);
			x := JSONElement->New(map_values);	
			x->ToString()->PrintLine();
			
		}
	}
}

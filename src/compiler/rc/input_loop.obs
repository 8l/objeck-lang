use IO;

bundle Default {
	class Test {
		function : Main(args : System.String[]) ~ Nil {
			Run();
		}

		function : native : Run() ~ Nil {
			f := FileReader->New("in.txt");
			if(f->IsOpen()) {
				string := f->ReadString();
				while(string->Size() > 0) {
					string->PrintLine();
					string := f->ReadString();
				};
				f->Close();
			};
		}			
	}
}

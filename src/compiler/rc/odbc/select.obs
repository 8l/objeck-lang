use ODBC;

bundle Default {
	class SqlTest {
		function : Main(args : String[]) ~ Nil {
			Run();
		}

		function : native : Run() ~ Nil {
			conn := Connection->New("test", "root", "");	
			if(conn <> Nil) {
				rs := conn->Select("select * from test.student");
				i := 0;	
				while(rs->Next()) {
					rs->GetVarchar("last_name")->PrintLine();
				};
				rs->Close();
				conn->Close();
			};
		}
	}
}

use JSON;

bundle Default {
	class Json {
		function : Main(args : String[]) ~ Nil {
			if(args->Size() = 1) {
				input := IO.FileReader->ReadFile(args[0]);
				if(input <> Nil) {
					parser := JSONParser->New(input);
					root := parser->Parse();
					if(root <> Nil) {
						if(root->GetType() = JSONType->OBJECT) {
							"parsed"->PrintLine();
						};	
					};
				};
			};
		}
	}
}

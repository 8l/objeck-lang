use Net;

bundle Default {
	class Hello {
		function : Main(args : System.String[]), Nil {
			socket := IPSocket->New("www.du.edu", 80);
			if(socket->IsOpen()) {
				buffer := Byte->New[256];
				socket->ReadBuffer(0, buffer->GetSize(), buffer);
				for(i := 0; i < buffer->GetSize(); i := i + 1;) {
					buffer[i]->PrintLine();
				};
				socket->Close();
			}
			else {
				"unable to open"->PrintLine();
			};
		}
	}
}

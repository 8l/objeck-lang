use Structure;

bundle Default {
	class Test {
		function : Main(args : String[]), Nil {
			Run();
		}

		function : native : Run(), Nil {		
			ints := IntVector->New();
			ints->AddBack(23);
			ints->AddBack(145);
			ints->AddBack(17);
			ints->AddBack(8);

			for(i := 0; i < ints->GetSize(); i := i + 1;) {
				ints->GetValue(i)->PrintLine();
			};
			
			ints->GetMin()->PrintLine();
			ints->GetMax()->PrintLine();
			ints->GetAverage()->PrintLine();

			ints->Sort();

			for(i := 0; i < ints->GetSize(); i := i + 1;) {
				ints->GetValue(i)->PrintLine();
			};
			

			"----------------"->PrintLine();

			floats := FloatVector->New();
			floats->AddBack(3.5);
			floats->AddBack(13.45);
			floats->AddBack(6.2);
			floats->AddBack(46.12);

			for(i := 0; i < floats->GetSize(); i := i + 1;) {
				floats->GetValue(i)->PrintLine();
			};

			floats->GetMin()->PrintLine();
			floats->GetMax()->PrintLine();
			floats->GetAverage()->PrintLine();
	
			floats->Sort();
				
			for(i := 0; i < floats->GetSize(); i := i + 1;) {
				floats->GetValue(i)->PrintLine();
			};
		}
	}
}

bundle Default {
	class Hello {
		function : Main(args : System.String[]), Nil {
			Run();
		}
	
		function : native : Run(), Nil {
			i := 0;
			do {
				i->PrintLine();
				i := i + 1;
			}	
			while(i < 0);
			"Bye"->PrintLine();

			s := "Hello World!";
			s->StartsWith("Hello")->PrintLine();
			s->EndsWith("World!")->PrintLine();


			3->Max(5)->PrintLine();
			"Hello World!"->StartsWith("Hello")->PrintLine();

		}
	}
}

bundle Default {
	class Hello {
		function : Main(args : System.String[]), Nil {
			i := 0;
			do {
				i->PrintLine();
				i := i + 1;
			}	
			while(i < 0);
			"Bye"->PrintLine();

			s := "Hello World!";
			s->StartsWith("Hello")->PrintLine();
			s->EndsWith("World!")->PrintLine();

			3->Max(5)->PrintLine();
		}
	}
}

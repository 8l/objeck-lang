bundle Default {
	class MyThread from Thread {
		New() {
			Parent();
		}

		method : public : Run(param : System.Base), Nil {
		}
	}

	class Hello {
		New() {
		}

		function : Main(args : System.String[]), Nil {
			Run();
		}

		function : Run(), Nil {
			t := MyThread->New();	
			t->Execute(Nil->As(Base));
		}
	}
}
